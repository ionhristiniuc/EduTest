<UserControl x:Class="StudentEduTest.ContentWindow.Panel.TestStatsPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StudentEduTest.ContentWindow.Panel"
             mc:Ignorable="d" 
             >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"></ColumnDefinition>
            <ColumnDefinition Width="150"></ColumnDefinition>
            <ColumnDefinition Width="100"></ColumnDefinition>
            <ColumnDefinition Width="100"></ColumnDefinition>
            <ColumnDefinition Width="100"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        
        <TextBlock Name="Name" Grid.Column="0"></TextBlock>
        <TextBlock Name="Course" Grid.Column="1"></TextBlock>
        <TextBlock Name="Status" Grid.Column="2"></TextBlock>
        <TextBlock Name="Date" Grid.Column="3"></TextBlock>
        <Button Name="CheckButton" Grid.Column="4" MaxWidth="75">Check</Button>
        
    </Grid>
</UserControl>
