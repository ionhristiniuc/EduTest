<Window x:Class="TeacherEduTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:contentMenu="clr-namespace:TeacherEduTest.ContentMenu"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0" Name="SideMenuGrid" Margin="5,5,5,5" />
        <Grid Grid.Column="1" Name="ContentMenuGrid">
            <contentMenu:MainMenuPanel x:Name="ContentMenuPanel"/>
        </Grid>
        
    </Grid>
</Window>
